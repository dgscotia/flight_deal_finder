def is_prime(x):
    if x <= 1:
        print x, "is not prime."
        return False
    elif x == 2:
        print x, "is prime."
        return True
    else:
        for n in range(2, x-1):
            if x % n == 0:
                print x, "is not prime."
                return False
        else:
            print x, "is prime."
            return True

for i in range (101):
    is_prime(i)
